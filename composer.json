{
    "name": "janakkapadia/commentable",
    "description": "Commentable - Easy way to apply comment on any model.",
    "type": "package",
    "license": "MIT",
    "authors": [{
        "name": "janakkapadia",
        "email": "kapadia.janak@gmail.com"
    }],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "JanakKapadia\\Commentable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JanakKapadia\\Commentable\\CommentableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
